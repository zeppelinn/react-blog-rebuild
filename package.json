{
  "name": "cortezx-react-blog",
  "version": "1.0.0",
  "description": "blog based on express/mongodb/react/mobx",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "rimraf dist",
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development node server/server.js",
    "build": "npm run clear && npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeppelinn/react-blog-rebuild.git"
  },
  "keywords": [
    "react-blog"
  ],
  "author": "lijun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zeppelinn/react-blog-rebuild/issues"
  },
  "homepage": "https://github.com/zeppelinn/react-blog-rebuild#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.19.0",
    "memory-fs": "^0.4.1",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  }
}
